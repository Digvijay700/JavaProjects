import java.util.Arrays;

public class mDArray {
    public static void main(String args[]) {
        int[][] matrix=new int[2][3];
        matrix[0][0]=1;
        System.out.println(Arrays.deepToString(matrix));

        int[][] mat={{1,2,3},{2,3,4}};
        System.out.println(Arrays.deepToString(mat));
    }
}
